set backspace=indent,eol,start
syntax on
set ruler
set number
set cursorline
set noexpandtab
set mouse=a
set tabstop=4
set shiftwidth=4
set encoding=utf-8
set clipboard=unnamed
set list lcs=tab:\|\ 
colorscheme molokai

let NERDTreeShowHidden=1
nmap <F7> :NERDTreeToggle<CR>
nmap <F8> :TagbarToggle<CR>
